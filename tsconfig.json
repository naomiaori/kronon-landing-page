{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "#/*": ["./*"],
      "#/public/*": ["./public/*"],
      "#/app/*": ["./src/app/*"],
      "#/components/*": ["./src/components/*"],
      "#/constants/*": ["./src/constants/*"],
      "#/hooks/*": ["./src/hooks/*"],
      "#/libs/*": ["./src/libs/*"],
      "#/actions/*": ["./src/actions/*"],
      "#/mock-data/*": ["./src/mock-data/*"],
      "#/providers/*": ["./src/providers/*"],
      "#/services/*": ["./src/services/*"],
      "#/stores/*": ["./src/stores/*"],
      "#/locales/*": ["./src/locales/*"],
      "#/styles/*": ["./src/styles/*"],
      "#/types/*": ["./src/types/*"],
      "#/utils/*": ["./src/utils/*"],
      "#/views/*": ["./src/views/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
